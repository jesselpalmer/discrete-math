{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:prettier/recommended", "google"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "indent": "off",
    "prettier/prettier": "error",
    "new-cap": ["error", {"capIsNewExceptions": ["Router"]}],
    "operator-linebreak": "off"
  }
}
